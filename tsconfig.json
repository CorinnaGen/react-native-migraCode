{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "jsx": "react",
    "baseUrl": ".",
    "noImplicitReturns": true,
    "noUncheckedIndexedAccess": false,
    "noFallthroughCasesInSwitch": true,
    "noImplicitOverride": true,
    "allowUnreachableCode": false,
    "paths": {
      "@components/*": [
        "./src/components/*"
      ],
      "@utils/*": [
        "./src/utils/*"
      ],
      "@styles/*": [
        "./src/styles/*"
      ],
      "@assets/*": [
        "./src/assets/*"
      ],
      "@icons/*": [
        "./src/assets/icons/*"
      ],
      "@pages/*": [
        "./src/pages/*"
      ],
      "@store/*": [
        "./src/store/*"
      ],
      "@core/*": [
        "./src/components/core/*"
      ],
      "@config/*": [
        "./src/config/*"
      ],
      "@api/*": [
        "./src/api/*"
      ],
      "@componentTypes/*": [
        "./src/types/*"
      ],
    },
  },
  "exclude": [
    "react-native.config.ts"
  ]
}